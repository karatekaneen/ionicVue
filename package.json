{
	"name": "Awake",
	"description": "Notifies the user when he/she is a specified distance from their destination to get some time to wake up and not wake up in Flen.",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"and": "vue-cli-service build && ionic capacitor copy android",
		"capacitor-copy": "vue-cli-service capacitor-copy",
		"capacitor-init": "vue-cli-service capacitor-init",
		"capacitor-open": "vue-cli-service capacitor-open",
		"capacitor-update": "vue-cli-service capacitor-update",
		"ionic:build": "vue-cli-service build",
		"ionic:serve": "vue-cli-service serve",
		"test:e2e": "vue-cli-service test:e2e",
		"test:unit": "vue-cli-service test:unit --verbose --silent",
		"testwatch": "vue-cli-service test:unit --verbose --silent --watchAll",
		"docs:md": "documentation build .\\src\\main.js --markdown-toc false -o ReadMe.md -f \"md\"",
		"docs:html": "documentation build .\\src\\main.js -o docs -f \"html\"",
		"docs:full": "npm run docs:md && npm run docs:html"
	},
	"dependencies": {
		"@capacitor/android": "^1.3.0",
		"@capacitor/core": "1.3.0",
		"@ionic/core": "^4.11.5",
		"@ionic/vue": "0.0.9",
		"axios": "^0.19.0",
		"core-js": "^3.3.2",
		"ionicons": "^4.6.3",
		"particles-bg-vue": "^1.1.1",
		"register-service-worker": "^1.6.2",
		"stencil-svgpaths-morphing": "^1.0.2",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3",
		"vue2-google-maps": "^0.10.7",
		"vuetify": "^2.1.0",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@capacitor/cli": "1.3.0",
		"@mdi/font": "^4.5.95",
		"@vue/cli-plugin-babel": "^4.0.5",
		"@vue/cli-plugin-e2e-cypress": "^4.0.5",
		"@vue/cli-plugin-eslint": "^4.0.5",
		"@vue/cli-plugin-pwa": "^4.0.5",
		"@vue/cli-plugin-unit-jest": "^4.0.5",
		"@vue/cli-service": "^4.0.5",
		"@vue/eslint-config-prettier": "^5.0.0",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-eslint": "^10.0.3",
		"documentation": "^12.1.4",
		"eslint": "^5.16.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-vue": "^5.0.0",
		"material-design-icons-iconfont": "^5.0.1",
		"prettier": "^1.19.1",
		"sass": "^1.19.0",
		"sass-loader": "^8.0.0",
		"vue-cli-plugin-pwa": "^1.0.0-alpha.1",
		"vue-cli-plugin-vuetify": "^2.0.2",
		"vue-template-compiler": "^2.6.10",
		"vuetify-loader": "^1.3.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
